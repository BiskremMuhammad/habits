{
  "expo": {
    "name": "Habit & Routine Tracker",
    "slug": "habit_routine_tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png"
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.biskrem.habits",
      "buildNumber": "1.0.2",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.biskrem.habits",
      "versionCode": 2,
      "googleServicesFile": "./google-services.json",
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "",
    "extra": {
      "eas": {
        "projectId": "2e09b0a7-2f6a-4f60-9aac-68538b31b9b3"
      }
    },
    "owner": "biskremmuhammad"
  }
}
